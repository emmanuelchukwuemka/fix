{
  "version": 2,
  "builds": [
    {
      "src": "wsgi.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.9",
        "maxLambdaSize": "15mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "wsgi.py"
    }
  ],
  "env": {
    "SECRET_KEY": "vercel-secret-key-change-in-dashboard",
    "JWT_SECRET_KEY": "vercel-jwt-secret-change-in-dashboard",
    "DATABASE_URL": "sqlite:///myfigpoint.db"
  }
}